extends ../aalayout/layout

block content
	.container.mt-4
		.row
			table.table.table-hover.table-bordered
				thead
					tr
						th 账户
						th delete
				tbody
					each Ader in Aders
						-a;
						tr(class="Ader-id-"+Ader._id)
							td #{Ader.code}
							td
								a.btn.btn-danger.del(href="/aderDel/"+Ader._id) delete